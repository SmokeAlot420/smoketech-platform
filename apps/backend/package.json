{
  "name": "viral-ai-content-system",
  "version": "1.0.0",
  "description": "AI-powered viral content generation and distribution system using Temporal orchestration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "worker": "tsx src/worker.ts",
    "workflow": "ts-node src/start-workflow.ts",
    "temporal:dev": "temporal server start-dev",
    "ui": "ts-node src/web-server.ts",
    "quotemoto": "ts-node src/start-quotemoto-workflow.ts",
    "quotemoto:savings": "ts-node src/start-quotemoto-workflow.ts --savings-only",
    "aria": "node dist/generate-single-aria.js",
    "aria-zho": "node dist/generate-aria-zho-enhanced.js",
    "aria-multi": "node dist/generate-aria-multi-angle.js",
    "aria-variants": "node dist/generate-aria-variants.js",
    "aria-reference": "node dist/generate-aria-multi-angle.js",
    "aria-base": "ts-node src/generate-aria-base-reference.ts",
    "aria-transform": "ts-node src/generate-aria-transformations.ts",
    "aria-video": "ts-node src/generate-aria-veo3-video.ts",
    "aria-consistency": "ts-node src/consistency-validator.ts",
    "quotemoto:dui": "ts-node src/start-quotemoto-workflow.ts --dui-only",
    "quotemoto:quick": "ts-node src/start-quotemoto-workflow.ts --quick-quote-only",
    "quotemoto:batch10": "ts-node src/start-quotemoto-workflow.ts --batch=10",
    "veo3:examples": "ts-node src/examples/veo3-production-example.ts",
    "veo3:basic": "ts-node src/examples/veo3-production-example.ts --example=1",
    "veo3:multi": "ts-node src/examples/veo3-production-example.ts --example=2",
    "veo3:4k": "ts-node src/examples/veo3-production-example.ts --example=3",
    "veo3:temporal": "ts-node src/examples/veo3-production-example.ts --example=4",
    "veo3:campaign": "ts-node src/examples/veo3-production-example.ts --example=5",
    "zho:veo3": "ts-node src/examples/zho-veo3-integration-examples.ts",
    "zho:basic": "ts-node src/examples/zho-veo3-integration-examples.ts --example=1",
    "zho:viral": "ts-node src/examples/zho-veo3-integration-examples.ts --example=2",
    "zho:corporate": "ts-node src/examples/zho-veo3-integration-examples.ts --example=3",
    "zho:eye-fix": "ts-node src/examples/zho-veo3-integration-examples.ts --example=4",
    "zho:branding": "ts-node src/examples/zho-veo3-integration-examples.ts --example=5",
    "zho:all": "ts-node src/examples/zho-veo3-integration-examples.ts --all",
    "veo3:quotemoto": "ts-node src/examples/zho-veo3-integration-examples.ts --example=1",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@google-cloud/storage": "^7.17.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.25.0",
    "@google/generative-ai": "^0.24.1",
    "@temporalio/activity": "^1.9.0",
    "@temporalio/client": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.4",
    "google-auth-library": "^10.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "langfuse": "^3.38.5",
    "mongoose": "^8.0.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "openai": "^6.3.0",
    "p-queue": "^9.0.0",
    "prom-client": "^15.0.0",
    "puppeteer": "^24.22.3",
    "rate-limiter-flexible": "^8.1.0",
    "redis": "^5.8.3",
    "replicate": "^1.2.0",
    "sharp": "^0.34.4",
    "stripe": "^19.1.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "google-auth-library": "^10.4.0",
    "gaxios": "^7.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "content-generation",
    "viral-content",
    "automation",
    "temporal",
    "workflow",
    "gemini",
    "veo3",
    "social-media",
    "tiktok",
    "instagram",
    "youtube"
  ],
  "author": "SmokeDev ðŸš¬",
